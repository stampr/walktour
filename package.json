{
  "name": "@stampr/walktour",
  "version": "5.2.0",
  "description": "Guided tour/walkthrough component for react",
  "type": "module",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "parcel demo/index.html --open",
    "build-demo": "parcel build demo/index.html --out-dir docs --out-file demo.html --public-url /walktour/",
    "prebuild": "rm -fr .parcel-cache",
    "build": "parcel build",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "build-docs": "npm run build-storybook && npm run build-demo",
    "prepublishOnly": "rm -rf dist && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/stampr/walktour.git"
  },
  "license": "MIT",
  "peerDependencies": {
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-tsc": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-knobs": "^5.2.5",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/addons": "^6.4.22",
    "@storybook/react": "^6.4.22",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.6",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "jest": "^27.4.5",
    "jsdom": "^19.0.0",
    "parcel": "^2.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "storybook": "^6.4.22",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.4"
  }
}
